            "winner2": "\u062c\u06cc\u0633\u06cc\u06a9\u0627 \u0648\u0627\u0646\u06cc\u0644\u0646",
            "coment2": "\u062f\u0627 \u0648\u0627\u0642\u0639\u06cc\u0627 \u06cc\u0648 \u0645\u0634\u0647\u0648\u0631 \u062c\u0646\u0631\u0627\u0644\u0633\u067c\u0648\u0631 \u067e\u0644\u0648\u0631\u0646\u0681\u06cc \u062f\u06cc \u060c \u06a9\u0685\u0648\u0693\u0647 \u0628\u0631\u06cc\u0627\u0644\u06cd \u0648\u0647 \u0686\u06d0 \u0645\u0627 \u062a\u0631\u0644\u0627\u0633\u0647 \u06a9\u0693\u06d0 \u060c \u0645\u062d\u0635\u0648\u0644 \u0627\u0635\u0644\u064a \u062f\u06cc \u060c \u0627\u0648 \u0645\u0627 \u0647\u06cc\u0685 \u0641\u06cc\u0633 \u0646\u0647 \u062f\u06cc \u0627\u062f\u0627 \u06a9\u0693\u06cc. \u0645\u0646\u0646\u0647.",
            "winner3": "\u0645\u06cc\u062a\u0648 \u0689\u06cc\u0648\u0689\u0633\u0646",
            "coment3": "\u067e\u0647 \u0644\u0648\u0645\u0693\u064a \u0633\u0631 \u06a9\u06d0 \u0645\u0627 \u067e\u062f\u06d0 \u0628\u0627\u0648\u0631 \u0646\u0647 \u062f\u0631\u0644\u0648\u062f \u060c \u0645\u06ab\u0631 \u0648\u0631\u0648\u0633\u062a\u0647 \u0644\u0647 \u0647\u063a\u0647 \u0686\u06d0 \u0645\u0627 \u062f\u0648\u06cc \u0633\u0631\u0647 \u0627\u0693\u06cc\u06a9\u0647 \u0648\u0646\u06cc\u0648\u0644\u0647 \u0646\u0648 \u062f\u0627 \u067e\u0647 \u0689\u0627\u06ab\u0647 \u0634\u0648\u0647 \u0686\u06d0 \u062f\u0627 \u06cc\u0648 \u0639\u0645\u0648\u0645\u064a \u0633\u067c\u0648\u0631 \u0648. \u0645\u0646\u0646\u0647 \u062c\u0646\u0631\u0627\u0644 \u0633\u067c\u0648\u0631 \u062f %s \u0644\u067e\u0627\u0631\u0647.",
            "winner4": "\u0648\u06cc\u0646\u06cc\u0633\u0627 \u0648\u06cc\u0644\u06cc\u0646",
            "coment4": "\u0632\u0645\u0627 \u062f \u0628\u062e\u062a \u0632\u06cc\u0627\u062a\u0648\u0627\u0644\u064a \u0644\u067e\u0627\u0631\u0647 \u062f \u062e\u062f\u0627\u06cc \u0685\u062e\u0647 \u0645\u0646\u0646\u0647 \u060c \u062f \u0648\u0631\u06a9\u0648\u0644\u0648 \u0685\u062e\u0647 \u0645\u0646\u0646\u0647 \u060c \u062c\u0646\u0631\u0627\u0644 \u0633\u067c\u0648\u0631 \u0632\u0647 \u06cc\u06d0 \u062e\u0648\u069a\u0648\u0645."
