    "section5": {
        "sTitleBefore5": "\u0627\u0633\u0627\u0646 \u062c\u064a \u0648\u0627\u0642\u0639\u0646 \u06fd \u06aa\u0627\u0631\u0648\u0628\u0627\u0631 \u062c\u064a \u0647\u0645\u0648\u0627\u0631 \u0647\u0644\u0646\u062f\u064a",
        "sTitle5": "\u062a\u0639\u0627\u0648\u0646 \u067f\u064a\u0644",
        "sDesc5": "\u0645\u06aa\u0645\u0644 \u0645\u062f\u062f \u0644\u0627\u0621\u0650 \u0627\u0633\u0627\u0646 \u062c\u064a \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u062c\u064a\u06aa\u0627 \u0647\u0645\u064a\u0634\u0647 \u0627\u0633\u0627\u0646 \u062c\u064a \u0645\u062f\u062f \u06aa\u0646\u062f\u064a",
        "tSupport1": "Amazon.com",
        "dSupport1": "\u0633\u064a\u067d\u0644 \u060c \u0648\u0627\u0634\u0646\u06af\u067d\u0646 \u06fe \u0642\u0627\u0626\u0645 \u0622\u0645\u0631\u064a\u06aa\u064a \u0645\u0644\u067d\u064a \u0646\u064a\u0634\u0646\u0644 \u067d\u064a\u06aa\u0646\u0627\u0644\u0627\u062c\u064a \u06aa\u0645\u067e\u0646\u064a \u060c \u062c\u064a\u06aa\u0627 \u0627\u064a \u06aa\u0627\u0645\u0631\u0633 \u060c \u06aa\u0644\u0627\u0626\u0648\u068a \u06aa\u0645\u067e\u064a\u0648\u067d\u0646\u06af \u060c \u068a\u062c\u064a\u067d\u0644 \u0627\u0633\u067d\u0631\u064a\u0645\u0646\u06af \u060c \u06fd \u0645\u0635\u0646\u0648\u0639\u064a \u0630\u0647\u0627\u0646\u062a \u062a\u064a \u068c\u064a\u0627\u0646 \u068f\u0626\u064a \u067f\u064a.",
        "tSupport2": "\u0627\u0648\u0648\u0631\u0633\u067d\u0631\u06aa. com",
        "dSupport2": "\u0622\u0645\u0631\u064a\u06aa\u064a \u0627\u0646\u067d\u0631\u0646\u064a\u067d \u062c\u0648 \u067e\u0631\u0686\u0648\u0646 \u0641\u0631\u0648\u0634 \u0628\u0646\u064a\u0627\u062f\u064a \u0637\u0648\u0631 \u062a\u064a \u0633\u0627\u0644\u067d Lake\u0646 City \u0634\u0647\u0631 \u062c\u064a \u0648\u064a\u062c\u0647\u0648 \u0645\u0627\u0626\u064a\u068a\u0644 \u060c \u064a\u0648\u067d\u0647 \u06fe \u0648\u0627\u0642\u0639 \u0622\u0647\u064a. \u067e\u064a\u067d\u0631\u06aa \u0627\u064a\u0645 \u0628\u0631\u0646 1999 \u06fe \u0627\u0648\u0648\u0631\u0633\u067d\u0631\u06aa. com \u0642\u0627\u0626\u0645 \u06aa\u064a\u0648.",
        "tSupport3": "\u0648\u0627\u0644\u0627\u0631\u067d. com",
        "dSupport3": "\u0622\u0645\u0631\u064a\u06aa\u0646 \u0645\u0644\u067d\u064a \u0646\u064a\u0634\u0646\u0644 \u067e\u0631\u0686\u0648\u0646 \u06aa\u0627\u0631\u067e\u0648\u0631\u064a\u0634\u0646 \u062c\u064a\u06aa\u0648 \u0622\u0645\u0631\u064a\u06aa\u0627 \u0645\u0627\u0646 \u0647\u0627\u0626\u067e\u0631 \u0645\u0627\u0631\u06aa\u064a\u067d \u060c \u0631\u0639\u0627\u064a\u062a\u064a \u068a\u067e\u0627\u0631\u067d\u0645\u0646\u067d \u0627\u0633\u067d\u0648\u0631\u0632 \u060c \u06fd \u06af\u0631\u0648\u0633 \u0627\u0633\u067d\u0648\u0631 \u062c\u0648 \u0647\u06aa \u0686\u064a\u0646\u0644 \u0647\u0644\u0627\u0626\u064a \u067f\u0648 \u060c \u062c\u0648 \u0628\u064a\u0646\u067d\u0648\u0646\u064a\u0644 \u060c \u0622\u0631\u06aa\u0646\u0633\u0627\u0633 \u06fe \u0648\u0627\u0642\u0639 \u0622\u0647\u064a.",
        "tSupport4": "BestBuy.com",
        "dSupport4": "\u0622\u0645\u0631\u064a\u06aa\u064a \u0645\u0644\u067d\u064a \u0646\u064a\u0634\u0646\u0644 \u06aa\u0646\u0633\u067d\u0631\u06aa\u0634\u0646 \u0627\u0644\u064a\u06aa\u067d\u0631\u0627\u0646\u06aa\u0633 \u067e\u0631\u0686\u0648\u0646 \u062c\u0648 \u0635\u062f\u0631 \u0645\u0642\u0627\u0645 \u0631\u0686 \u0641\u064a\u0644\u068a \u060c \u0645\u064a\u0646\u064a\u0633\u0648\u067d\u0627 \u06fe \u0622\u0647\u064a. \u0627\u0647\u0648 \u0627\u0635\u0644 \u06fe \u0631\u0686\u0631\u068a \u0627\u064a\u0645 \u0634\u0648\u0644\u0632 \u06fd \u062c\u064a\u0645\u0633 \u0648\u064a\u0644\u0631 \u067e\u0627\u0631\u0627\u0646 1966 \u06fe \u0642\u0627\u0626\u0645 \u06aa\u064a\u0648 \u0648\u064a\u0648 \u062c\u064a\u06aa\u0648 \u0647\u06aa \u0622\u068a\u064a\u0648 \u0627\u0633\u067e\u064a\u0634\u0644\u0633\u067d \u0627\u0633\u067d\u0648\u0631 \u0637\u0648\u0631 \u0622\u0648\u0627\u0632 \u062c\u0648 \u0645\u064a\u0648\u0632\u06aa \u0633\u068f\u0628\u0648 \u0622\u0647\u064a.",
        "tSupport5": "\u0639\u0644\u064a \u06af\u064a\u0627",
        "dSupport5": "\u0686\u064a\u0646 \u06fe \u0622\u0646 \u0644\u0627\u0626\u0646 \u062e\u0648\u0631\u062f\u06c1 \u0633\u0631\u0648\u0633 \u062c\u0646\u0647\u0646 \u062c\u064a \u0645\u0644\u06aa\u064a\u062a \u0639\u0644\u064a \u0628\u0627\u0628\u0627 \u06af\u0631\u0648\u067e \u062c\u064a \u0645\u0644\u06aa\u064a\u062a \u0622\u0647\u064a. 2010 \u06fe \u0634\u0631\u0648\u0639 \u067f\u064a\u0648 \u060c \u0627\u0647\u0648 \u0686\u064a\u0646 \u06fd \u067b\u064a\u0646 \u0647\u0646\u068c\u0646 \u062c\u0647\u0699\u0648\u06aa \u0633\u0646\u06af\u0627\u067e\u0648\u0631 \u06fe \u0646\u0646 businesses\u0646 \u06aa\u0627\u0631\u0648\u0628\u0627\u0631 \u0645\u0627\u0646 \u067a\u0647\u064a\u0644 \u0622\u0647\u064a \u060c \u062c\u064a\u06aa\u064a \u0628\u064a\u0646 \u0627\u0644\u0627\u0642\u0648\u0627\u0645\u064a \u0622\u0646\u0644\u0627\u0626\u0646 \u062e\u0631\u064a\u062f\u0627\u0631\u0646 \u06a9\u064a \u0645\u0635\u0646\u0648\u0639\u0627\u062a \u067e\u064a\u0634 \u06aa\u0646 \u067f\u0627.",
        "tSupport6": "eay.com",
        "dSupport6": "\u0622\u0645\u0631\u064a\u06aa\u064a \u0645\u0644\u067d\u064a \u0646\u064a\u0634\u0646\u0644 \u0627\u064a \u06aa\u0627\u0645\u0631\u0633 \u06aa\u0627\u0631\u067e\u0648\u0631\u064a\u0634\u0646 \u0633\u0627\u0646 \u062c\u0648\u0633 \u060c \u06aa\u064a\u0644\u064a\u0641\u0648\u0631\u0646\u064a\u0627 \u06fe \u0642\u0627\u0626\u0645 \u060c \u062c\u064a\u06aa\u0627 \u067e\u0646\u0647\u0646\u062c\u064a \u0648\u064a\u0628 \u0633\u0627\u0626\u064a\u067d \u062c\u064a \u0630\u0631\u064a\u0639\u064a \u0635\u0627\u0631\u0641 \u06a9\u0627\u0646 \u0635\u0627\u0631\u0641 \u06fd \u0648\u0627\u067e\u0627\u0631 \u06a9\u0627\u0646 \u0648\u067a\u064a \u06af\u0631\u0627\u0647\u06aa\u0646 \u062a\u0627\u0626\u064a\u0646 \u067e\u0647\u0686\u0627\u0626\u06bb \u062c\u064a \u0633\u0647\u0648\u0644\u062a \u068f\u064a\u0646\u062f\u064a"
    },
