    "section3": {
        "sTitleBefore3": "\uc9c0\uae08 \uc0ac\uc138\uc694",
        "sTitle3": "\uac00\uc785\ud558\ub294 \ubc29\ubc95?",
        "sDesc3": "24 \uc2dc\uac04 \uc774\ub0b4\uc5d0 \ubb34\ub8cc GiveAway\ub97c \ubc1b\uc73c\uc138\uc694! \uc774 \ub4dc\ubb38 \uae30\ud68c\ub97c \ub193\uce58\uc9c0 \ub9c8\uc138\uc694. \ub9e4\uc6b0 \uc27d\uc2b5\ub2c8\ub2e4. \uc800\ud76c \uc6f9 \uc0ac\uc774\ud2b8\uc5d0 \ub4f1\ub85d\ud558\uace0 \uc124\ubb38 \uc870\uc0ac\uc5d0 \ucc38\uc5ec\ud558\uc138\uc694 (\uc788\ub294 \uacbd\uc6b0). \ube44\uc6a9\uc744 \uc9c0\ubd88 \ud560 \ud544\uc694\uac00 \uc5c6\uc2b5\ub2c8\ub2e4. \ucd95\ud558\ud569\ub2c8\ub2e4!",
        "step1": {
            "sStep1": "1 \ub2e8\uacc4",
            "ssDesc11": " %s  \ub2f9\ucca8 \uae30\ud68c\ub97c \uc5bb\uc73c\ub824\uba74 \uacc4\uc815 \ub4f1\ub85d \uc591\uc2dd\uc744 \uc791\uc131\ud558\uae30 \ub9cc\ud558\uba74\ub429\ub2c8\ub2e4. \uc2dc\uc791\ud558\ub824\uba74 \uc774\uba54\uc77c \uc8fc\uc18c\ub85c \ucf58\ud14c\uc2a4\ud2b8\uc5d0 \ucc38\uac00\ud558\uc138\uc694."
        },
        "step2": {
            "sStep2": "2 \ub2e8\uacc4",
            "ssDesc21": "\ub85c\uadf8\uc778\ud558\uba74 \uc18c\uc15c \ubbf8\ub514\uc5b4 \ucc44\ub110 \ubc29\ubb38\uc744 \ud3ec\ud568\ud558\uc5ec \ub2e4\uc591\ud55c \uc791\uc5c5\uc744 \uc644\ub8cc\ud558\uc5ec \ucd94\uac00 \ud56d\ubaa9\uc744 \uc5bb\uc744 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
            "ssDesc22": "\uc6d0\ud558\ub294\ub9cc\ud07c\uc758 \uc791\uc5c5\uc744 \uc644\ub8cc \ud560 \uc218 \uc788\uc73c\uba70 \uac01 \uc791\uc5c5\uc740 \uace0\uc720 \ud55c \uc218\uc758 \ud56d\ubaa9\uc744 \uc81c\uacf5\ud558\uba70 \uac01 \ud56d\ubaa9\uc740 \uc7a0\uc7ac\uc801 \uc778 \uc6b0\uc2b9\uc790\uac00\ub429\ub2c8\ub2e4.",
            "ssDesc23": "\ub2e8 $ 1\ub85c \uc791\uc5c5\uc744 \uac74\ub108 \ub6f8 \uc218\ub3c4 \uc788\uc2b5\ub2c8\ub2e4."
        },
        "step3": {
            "done": "\ub05d\ub09c.",
            "sStep3": "3 \ub2e8\uacc4",
            "ssDesc31": "\ub05d\ub09c. \ub2f9\uc0ac \uc0ac\uc774\ud2b8\uc5d0 \ub4f1\ub85d \ud560 \ub54c \uc785\ub825 \ud55c \uc5f0\ub77d\ucc98\ub97c \uae30\ubc18\uc73c\ub85c \uc989\uc2dc \uc5f0\ub77d\uc744 \ub4dc\ub9b4 \uac83\uc785\ub2c8\ub2e4."
        }
    },
    "banner3": {
        "bTitle3": "\ud070 \ubc29\ubc95\uc73c\ub85c  %s \uc5d0\uac8c \uc918.",
        "bDesc3": "\uc774 \uc774\ubca4\ud2b8\ub294 \uc791\ub144\ubcf4\ub2e4 \ub354 \ub9ce\uc740 \uc6b0\uc2b9 \uc2ac\ub86f\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4. \ub354 \ud070 \uae30\ud68c\ub97c \uc704\ud574 \uce5c\uad6c\uc640 \uac00\uc871\uc744 \ucd08\ub300\ud558\uc2ed\uc2dc\uc624. \uc774 \uc774\ubca4\ud2b8\uac00 \uc2dc\uc791\ub418\uae30 \uc804\uc5d0 <a href='%s' target='_blank'> \uc9c0\uae08 \ub4f1\ub85d <\/a>\ud558\uc138\uc694."
    },
    "section4": {
        "sTitleBefore4": "\uc120\ud0dd\ub41c \ud3c9\uac00",
        "sTitle4": "\uacbd\ud488 \uc218\uc0c1\uc790",
        "sDesc4": "\uacbd\ud488 \uc218\uc0c1\uc790\uc5d0 \ub300\ud55c \uc77c\ubd80 \ub9ac\ubdf0",
        "theWinner": {
            "winner": "\uc6b0\uc2b9\uc790",
            "winner1": "\uc800\uc2a4\ud2f4 \ub2e4\ub2c8\uc5d8",
            "coment1": "\ud604\uae08 \uac10\uc0ac\ud569\ub2c8\ub2e4. \ub9e4\uc7a5\uc774 \ub354 \ubd90\ube44\uae30\ub97c \ubc14\ub78d\ub2c8\ub2e4.",
            "winner2": "\uc81c\uc2dc\uce74 \ubc18 \ud5ec\ub978",
            "coment2": "\uc774\uac83\uc740 \uc815\ub9d0 \uc720\uba85\ud55c \uc7a1\ud654\uc810 \uc0c1\uc810\uc774\uace0 \ud328\ud0a4\uc9c0\ub294 \uc131\uacf5\uc801\uc73c\ub85c \ubc1b\uc558\uc73c\uba70 \uc81c\ud488\uc740 \uc6d0\ubcf8\uc774\uba70 \uc218\uc218\ub8cc\ub97c \uc9c0\ubd88\ud558\uc9c0 \uc54a\uc558\uc2b5\ub2c8\ub2e4. \uac10\uc0ac\ud569\ub2c8\ub2e4.",
            "winner3": "\ub9e4\ud29c \ub370\uc774\ube44\uc2a8",
            "coment3": "\ucc98\uc74c\uc5d0\ub294 \ubbff\uc9c0 \ubabb\ud588\uc9c0\ub9cc \uc5f0\ub77d\uc744 \ucde8\ud55c \ud6c4 \uc77c\ubc18 \uc0c1\uc810 \uc778 \uac83\uc73c\ub85c \ubc1d\ud600\uc84c\uc2b5\ub2c8\ub2e4.  %s \uc5d0 \ub300\ud55c GeneralStore\uc5d0 \uac10\uc0ac\ub4dc\ub9bd\ub2c8\ub2e4.",
            "winner4": "\ubc14\ub124\uc0ac \uc6f0 \ud5e4\uc778",
            "coment4": "\ub0b4 \ud589\uc6b4\uc744 \ub192\uc5ec \uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4. \uacbd\ud488\uc744 \uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4. GeneralStore \ub098\ub294 \uadf8\uac83\uc744 \uc88b\uc544\ud569\ub2c8\ub2e4."
        }
    },
    "section5": {
        "sTitleBefore5": "\uc774\ubca4\ud2b8 \ubc0f \ube44\uc988\ub2c8\uc2a4\uc758 \uc6d0\ud65c\ud55c \uc6b4\uc601",
        "sTitle5": "\uc9c0\uc6d0",
        "sDesc5": "\ud56d\uc0c1 \uc6b0\ub9ac\ub97c \ub3c4\uc640\uc8fc\ub294 \uc804\ud3ed\uc801 \uc778 \uc9c0\uc6d0\uc5d0 \uac10\uc0ac\ub4dc\ub9bd\ub2c8\ub2e4.",
        "tSupport1": "Amazon.com",
        "dSupport1": "\uc804\uc790 \uc0c1\uac70\ub798, \ud074\ub77c\uc6b0\ub4dc \ucef4\ud4e8\ud305, \ub514\uc9c0\ud138 \uc2a4\ud2b8\ub9ac\ubc0d \ubc0f \uc778\uacf5 \uc9c0\ub2a5\uc5d0 \uc911\uc810\uc744 \ub454 \uc6cc\uc2f1\ud134 \uc8fc \uc2dc\uc560\ud2c0\uc5d0 \ubcf8\uc0ac\ub97c \ub454 \ubbf8\uad6d\uc758 \ub2e4\uad6d\uc801 \uae30\uc220 \ud68c\uc0ac\uc785\ub2c8\ub2e4.",
        "tSupport2": "Overstock.com",
        "dSupport2": "\uc8fc\ub85c \uac00\uad6c\ub97c \ud310\ub9e4\ud558\ub294 \ubbf8\uad6d \uc778\ud130\ub137 \uc18c\ub9e4 \uc5c5\uccb4\ub294 \uc194\ud2b8 \ub808\uc774\ud06c \uc2dc\ud2f0 \uadfc\ucc98 \uc720\ud0c0\uc8fc \ubbf8\ub4dc \ubca0\uc77c\uc5d0 \ubcf8\uc0ac\ub97c\ub450\uace0 \uc788\uc2b5\ub2c8\ub2e4. Patrick M. Byrne\uc740 1999 \ub144\uc5d0 Overstock.com\uc744 \uc124\ub9bd\ud588\uc2b5\ub2c8\ub2e4.",
        "tSupport3": "Walmart.com",
        "dSupport3": "\ubbf8\uad6d\uc758 \ub300\ud615 \uc288\ud37c\ub9c8\ucf13, \ud560\uc778 \ubc31\ud654\uc810 \ubc0f \uc2dd\ub8cc\ud488 \uc810 \uccb4\uc778\uc744 \uc6b4\uc601\ud558\ub294 \ubbf8\uad6d\uc758 \ub2e4\uad6d\uc801 \uc18c\ub9e4 \uae30\uc5c5\uc73c\ub85c \uc544\uce78\uc18c \uc8fc \ubca4 \ud1a4\ube4c\uc5d0 \ubcf8\uc0ac\ub97c\ub450\uace0 \uc788\uc2b5\ub2c8\ub2e4.",
        "tSupport4": "BestBuy.com",
        "dSupport4": "\ubbf8\ub124\uc18c\ud0c0 \uc8fc \ub9ac\uce58 \ud544\ub4dc\uc5d0 \ubcf8\uc0ac\ub97c \ub454 \ubbf8\uad6d\uc758 \ub2e4\uad6d\uc801 \uc18c\ube44\uc790 \uac00\uc804 \uc18c\ub9e4 \uc5c5\uccb4. \uc6d0\ub798\ub294 Richard M. Schulze\uc640 James Wheeler\uac00 1966 \ub144 Sound of Music\uc774\ub77c\ub294 \uc624\ub514\uc624 \uc804\ubb38\uc810\uc73c\ub85c \uc124\ub9bd\ud588\uc2b5\ub2c8\ub2e4.",
        "tSupport5": "AliExpress.com",
        "dSupport5": "Alibaba Group\uc774 \uc18c\uc720 \ud55c \uc911\uad6d\uc5d0 \uae30\ubc18\uc744 \ub454 \uc628\ub77c\uc778 \uc18c\ub9e4 \uc11c\ube44\uc2a4. 2010 \ub144\uc5d0 \uc2dc\uc791\ub41c\uc774 \ud68c\uc0ac\ub294 \uc911\uad6d \ubc0f \uc2f1\uac00\ud3ec\ub974\uc640 \uac19\uc740 \uae30\ud0c0 \uc9c0\uc5ed\uc758 \uc18c\uaddc\ubaa8 \ube44\uc988\ub2c8\uc2a4\ub85c \uad6c\uc131\ub418\uc5b4 \uc788\uc73c\uba70 \ud574\uc678 \uc628\ub77c\uc778 \uad6c\ub9e4\uc790\uc5d0\uac8c \uc81c\ud488\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.",
        "tSupport6": "Ebay.com",
        "dSupport6": "\uce98\ub9ac\ud3ec\ub2c8\uc544 \uc0b0\ud638\uc138\uc5d0 \ubcf8\uc0ac\ub97c \ub454 \ubbf8\uad6d\uc758 \ub2e4\uad6d\uc801 \uc804\uc790 \uc0c1\uac70\ub798 \uae30\uc5c5\uc73c\ub85c \uc6f9 \uc0ac\uc774\ud2b8\ub97c \ud1b5\ud574 \uc18c\ube44\uc790 \ub300 \uc18c\ube44\uc790 \ubc0f B2C \ud310\ub9e4\ub97c \ucd09\uc9c4\ud569\ub2c8\ub2e4."
    },
