    "banner3": {
        "bTitle3": " %s \u0dc0\u0dd2\u0dc1\u0dcf\u0dbd \u0dbd\u0dd9\u0dc3 \u0dbd\u0db6\u0dcf \u0daf\u0dd9\u0db1\u0dca\u0db1.",
        "bDesc3": "\u0db8\u0dd9\u0db8 \u0dc3\u0dd2\u0daf\u0dd4\u0dc0\u0dd3\u0db8 \u0db4\u0dc3\u0dd4\u0d9c\u0dd2\u0dba \u0dc0\u0dc3\u0dbb\u0da7 \u0dc0\u0da9\u0dcf \u0da2\u0dba\u0d9c\u0dca\u200d\u0dbb\u0dcf\u0dc4\u0dd3 \u0dc3\u0dca\u0dae\u0dcf\u0db1 \u0dbd\u0db6\u0dcf \u0daf\u0dd9\u0dba\u0dd2. \u0d8a\u0da7\u0dad\u0dca \u0dc0\u0da9\u0dcf \u0dc0\u0dd2\u0dc1\u0dcf\u0dbd \u0d85\u0dc0\u0dc3\u0dca\u0dae\u0dcf\u0dc0\u0db1\u0dca \u0dc3\u0db3\u0dc4\u0dcf \u0d94\u0db6\u0dda \u0db8\u0dd2\u0dad\u0dd4\u0dbb\u0db1\u0dca\u0da7 \u0dc3\u0dc4 \u0db4\u0dc0\u0dd4\u0dbd\u0dda \u0d85\u0dba\u0da7 \u0d86\u0dbb\u0dcf\u0db0\u0db1\u0dcf \u0d9a\u0dbb\u0db1\u0dca\u0db1. \u0db8\u0dd9\u0db8 \u0dc3\u0dd2\u0daf\u0dd4\u0dc0\u0dd3\u0db8 \u0d86\u0dbb\u0db8\u0dca\u0db7 \u0dc0\u0dd3\u0db8\u0da7 \u0db4\u0dd9\u0dbb <a href='%s' target='_blank'> \u0daf\u0dd0\u0db1\u0dca \u0dbd\u0dd2\u0dba\u0dcf\u0db4\u0daf\u0dd2\u0d82\u0da0\u0dd2 \u0dc0\u0db1\u0dca\u0db1 <\/a>."
    },
    "section4": {
        "sTitleBefore4": "\u0dad\u0ddd\u0dbb\u0dcf\u0d9c\u0dad\u0dca \u0dad\u0d9a\u0dca\u0dc3\u0dda\u0dbb\u0dd4\u0dc0",
        "sTitle4": "\u0daf\u0dd3\u0db8\u0db1\u0dcf \u0da2\u0dba\u0d9c\u0dca\u200d\u0dbb\u0dcf\u0dc4\u0d9a\u0dba\u0dcf",
        "sDesc4": "\u0daf\u0dd3\u0db8\u0db1\u0dcf \u0dbd\u0db6\u0dcf \u0daf\u0dd9\u0db1 \u0da2\u0dba\u0d9c\u0dca\u200d\u0dbb\u0dcf\u0dc4\u0d9a\u0dba\u0dd2\u0db1\u0dca \u0db4\u0dd2\u0dc5\u0dd2\u0db6\u0db3 \u0dc3\u0db8\u0dc4\u0dbb \u0dc3\u0db8\u0dcf\u0dbd\u0ddd\u0da0\u0db1",
        "theWinner": {
            "winner": "\u0dc0\u0dd2\u0db1\u0dca\u0db1\u0dbb\u0dca",
            "winner1": "\u0da2\u0dc3\u0dca\u0da7\u0dd2\u0db1\u0dca \u0da9\u0dd0\u0db1\u0dd2\u0dba\u0dd9\u0dbd\u0dca",
            "coment1": "\u0db8\u0dd4\u0daf\u0dbd\u0dca \u0dc3\u0db3\u0dc4\u0dcf \u0dc3\u0dca\u0dad\u0dd6\u0dad\u0dd2\u0dba\u0dd2, \u0d94\u0db6\u0dda \u0d9c\u0db6\u0da9\u0dcf\u0dc0 \u0dc0\u0da9\u0dcf\u0dad\u0dca \u0da2\u0db1\u0dcf\u0d9a\u0dd3\u0dbb\u0dca\u0dab \u0dc0\u0db1\u0dd4 \u0d87\u0dad\u0dd0\u0dba\u0dd2 \u0db8\u0db8 \u0db6\u0dbd\u0dcf\u0db4\u0ddc\u0dbb\u0ddc\u0dad\u0dca\u0dad\u0dd4 \u0dc0\u0dd9\u0db8\u0dd2.",
            "winner2": "\u0da2\u0dd9\u0dc3\u0dd2\u0d9a\u0dcf \u0dc0\u0dd0\u0db1\u0dca\u0dc4\u0dd9\u0dbd\u0dca\u0db1\u0dca",
            "coment2": "\u0db8\u0dd9\u0dba \u0dc3\u0dd0\u0db6\u0dc0\u0dd2\u0db1\u0dca\u0db8 \u0db4\u0dca\u200d\u0dbb\u0dc3\u0dd2\u0daf\u0dca\u0db0 \u0da2\u0dd9\u0db1\u0dbb\u0dbd\u0dca \u0dc3\u0dca\u0da7\u0ddd\u0dbb\u0dca \u0dc3\u0dcf\u0db4\u0dca\u0db4\u0dd4\u0dc0\u0d9a\u0dd2, \u0db4\u0dd0\u0d9a\u0dda\u0da2\u0dba \u0db8\u0da7 \u0dbd\u0dd0\u0db6\u0dd4\u0dab\u0dd2, \u0db1\u0dd2\u0dc2\u0dca\u0db4\u0dcf\u0daf\u0dd2\u0dad\u0dba \u0db8\u0dd4\u0dbd\u0dca \u0db4\u0dd2\u0da7\u0db4\u0dad\u0d9a\u0dca \u0dc0\u0db1 \u0d85\u0dad\u0dbb \u0db8\u0db8 \u0d9a\u0dd2\u0dc3\u0dd2\u0daf\u0dd4 \u0d9c\u0dcf\u0dc3\u0dca\u0dad\u0dd4\u0dc0\u0d9a\u0dca \u0d9c\u0dd9\u0dc0\u0dcf \u0db1\u0dd0\u0dad. \u0d94\u0db6\u0da7 \u0dc3\u0dca\u0dad\u0dd4\u0dad\u0dd2\u0dba\u0dd2.",
            "winner3": "\u0db8\u0dd0\u0dad\u0dd2\u0dc0\u0dca \u0da9\u0dda\u0dc0\u0dd2\u0da9\u0dca\u0dc3\u0db1\u0dca",
            "coment3": "\u0db8\u0dd4\u0dbd\u0daf\u0dd3 \u0db8\u0db8 \u0d91\u0dba \u0dc0\u0dd2\u0dc1\u0dca\u0dc0\u0dcf\u0dc3 \u0d9a\u0dc5\u0dda \u0db1\u0dd0\u0dad, \u0db1\u0db8\u0dd4\u0dad\u0dca \u0db8\u0db8 \u0d94\u0dc0\u0dd4\u0db1\u0dca \u0dc3\u0db8\u0dca\u0db6\u0db1\u0dca\u0db0 \u0d9a\u0dbb \u0d9c\u0dd0\u0db1\u0dd3\u0db8\u0dd9\u0db1\u0dca \u0db4\u0dc3\u0dd4\u0dc0 \u0d91\u0dba \u0da2\u0dd9\u0db1\u0dbb\u0dbd\u0dca \u0dc3\u0dca\u0da7\u0ddd\u0dbb\u0dca \u0d91\u0d9a\u0d9a\u0dca \u0db6\u0dc0 \u0db4\u0dd9\u0db1\u0dd3 \u0d9c\u0dd2\u0dba\u0dda\u0dba.  %s \u0dc3\u0db3\u0dc4\u0dcf \u0da2\u0dd9\u0db1\u0dbb\u0dbd\u0dca \u0dc3\u0dca\u0da7\u0ddd\u0dbb\u0dca\u0da7 \u0dc3\u0dca\u0dad\u0dd6\u0dad\u0dd2\u0dba\u0dd2.",
            "winner4": "\u0dc0\u0dd0\u0db1\u0dda\u0dc3\u0dcf \u0dc0\u0dd9\u0dbd\u0dca\u0dc4\u0dba\u0dd2\u0db1\u0dca",
            "coment4": "\u0db8\u0d9c\u0dda \u0dc0\u0dcf\u0dc3\u0db1\u0dcf\u0dc0 \u0dc0\u0dd0\u0da9\u0dd2 \u0d9a\u0dd2\u0dbb\u0dd3\u0db8 \u0d9c\u0dd0\u0db1 \u0daf\u0dd9\u0dc0\u0dd2\u0dba\u0db1\u0dca\u0da7 \u0dc3\u0dca\u0dad\u0dd6\u0dad\u0dd2\u0dba\u0dd2, \u0dbd\u0db6\u0dcf \u0daf\u0dd3\u0db8 \u0d9c\u0dd0\u0db1 \u0dc3\u0dca\u0dad\u0dd6\u0dad\u0dd2\u0dba\u0dd2, \u0da2\u0dd9\u0db1\u0dbb\u0dbd\u0dca \u0dc3\u0dca\u0da7\u0ddd\u0dbb\u0dca \u0db8\u0db8 \u0d91\u0dba\u0da7 \u0d9a\u0dd0\u0db8\u0dad\u0dd2\u0dba\u0dd2."
        }
    },
    "section5": {
        "sTitleBefore5": "\u0d85\u0db4\u0d9c\u0dda \u0dc3\u0dd2\u0daf\u0dd4\u0dc0\u0dd3\u0db8\u0dca \u0dc3\u0dc4 \u0dc0\u0dca\u200d\u0dba\u0dcf\u0db4\u0dcf\u0dbb \u0d9a\u0da7\u0dba\u0dd4\u0dad\u0dd4 \u0dc0\u0dd2\u0db0\u0dd2\u0db8\u0dad\u0dca\u0dc0 \u0db4\u0dc0\u0dad\u0dca\u0dc0\u0dcf\u0d9c\u0dd9\u0db1 \u0dba\u0dcf\u0db8",
        "sTitle5": "\u0dc3\u0dc4\u0dcf\u0dba \u0daf\u0d9a\u0dca\u0dc0\u0dba\u0dd2",
        "sDesc5": "\u0dc3\u0dd0\u0db8\u0dc0\u0dd2\u0da7\u0db8 \u0d85\u0db4\u0da7 \u0d8b\u0db4\u0d9a\u0dcf\u0dbb \u0d9a\u0dbb\u0db1 \u0db4\u0dd6\u0dbb\u0dca\u0dab \u0dc3\u0dc4\u0dba\u0ddd\u0d9c\u0dba\u0da7 \u0d85\u0db4\u0d9c\u0dda \u0dc3\u0dca\u0dad\u0dd6\u0dad\u0dd2\u0dba\u0dd2",
        "tSupport1": "Amazon.com",
        "dSupport1": "\u0d8a-\u0dc0\u0dcf\u0dab\u0dd2\u0da2\u0dca\u200d\u0dba\u0dba, \u0dc0\u0dbd\u0dcf\u0d9a\u0dd4\u0dc5\u0dd4 \u0db4\u0dbb\u0dd2\u0d9c\u0dab\u0d9a\u0d9a\u0dbb\u0dab\u0dba, \u0da9\u0dd2\u0da2\u0dd2\u0da7\u0dbd\u0dca \u0db4\u0dca\u200d\u0dbb\u0dc0\u0dcf\u0dc4\u0dba \u0dc3\u0dc4 \u0d9a\u0dd8\u0dad\u0dd2\u0db8 \u0db6\u0dd4\u0daf\u0dca\u0db0\u0dd2\u0dba \u0d9a\u0dd9\u0dbb\u0dd9\u0dc4\u0dd2 \u0d85\u0dc0\u0db0\u0dcf\u0db1\u0dba \u0dba\u0ddc\u0db8\u0dd4 \u0d9a\u0dbb\u0db1 \u0dc0\u0ddc\u0dc2\u0dd2\u0d82\u0da7\u0db1\u0dba\u0dda \u0dc3\u0dd2\u0dba\u0dd0\u0da7\u0dbd\u0dca \u0dc4\u0dd2 \u0db4\u0dd2\u0dc4\u0dd2\u0da7\u0dd2 \u0d87\u0db8\u0dbb\u0dd2\u0d9a\u0dcf\u0db1\u0dd4 \u0db6\u0dc4\u0dd4\u0da2\u0dcf\u0dad\u0dd2\u0d9a \u0dad\u0dcf\u0d9a\u0dca\u200d\u0dc2\u0dab \u0dc3\u0db8\u0dcf\u0d9c\u0db8.",
        "tSupport2": "Overstock.com",
        "dSupport2": "\u0d87\u0db8\u0dbb\u0dd2\u0d9a\u0dcf\u0db1\u0dd4 \u0d85\u0db1\u0dca\u0dad\u0dbb\u0dca\u0da2\u0dcf\u0dbd \u0dc3\u0dd2\u0dbd\u0dca\u0dbd\u0dbb \u0dc0\u0dd9\u0dc5\u0dd9\u0db1\u0dca\u0daf\u0dd9\u0d9a\u0dd4 \u0db8\u0dd6\u0dbd\u0dd2\u0d9a \u0dc0\u0dc1\u0dba\u0dd9\u0db1\u0dca \u0d9c\u0dd8\u0dc4 \u0db7\u0dcf\u0dab\u0dca\u0da9 \u0d85\u0dbd\u0dd9\u0dc0\u0dd2 \u0d9a\u0dbb\u0db1\u0dca\u0db1\u0dda \u0dc3\u0ddd\u0dbd\u0dca\u0da7\u0dca \u0dbd\u0dda\u0d9a\u0dca \u0dc3\u0dd2\u0da7\u0dd2 \u0d85\u0dc3\u0dbd t \u0da7\u0dcf \u0dc4\u0dd2 \u0db8\u0dd2\u0da9\u0dca\u0dc0\u0dda\u0dbd\u0dca \u0dc4\u0dd2 \u0dba. \u0db4\u0dd0\u0da7\u0dca\u200d\u0dbb\u0dd2\u0d9a\u0dca \u0d91\u0db8\u0dca. \u0db6\u0dbb\u0dca\u0db1\u0dca 1999 \u0daf\u0dd3 Overstock.com \u0d86\u0dbb\u0db8\u0dca\u0db7 \u0d9a\u0dc5\u0dda\u0dba.",
