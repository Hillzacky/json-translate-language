        "dSupport5": "\u0626\u06d5\u0644\u0649 \u0628\u0627\u0628\u0627 \u06af\u06c7\u0631\u06c7\u06be\u0649\u0646\u0649\u06ad \u0626\u0649\u06af\u0649\u062f\u0627\u0631\u0686\u0649\u0644\u0649\u0642\u0649\u062f\u0649\u0643\u0649 \u062c\u06c7\u06ad\u06af\u0648\u062f\u0649\u0643\u0649 \u062a\u0648\u0631 \u067e\u0627\u0631\u0686\u06d5 \u0633\u06d0\u062a\u0649\u0634 \u0645\u06c7\u0644\u0627\u0632\u0649\u0645\u0649\u062a\u0649. \u0626\u06c7 2010-\u064a\u0649\u0644\u0649 \u064a\u0648\u0644\u063a\u0627 \u0642\u0648\u064a\u06c7\u0644\u063a\u0627\u0646 \u0628\u0648\u0644\u06c7\u067e \u060c \u062c\u06c7\u06ad\u06af\u0648 \u06cb\u06d5 \u0633\u0649\u0646\u06af\u0627\u067e\u0648\u0631 \u0642\u0627\u062a\u0627\u0631\u0644\u0649\u0642 \u0628\u0627\u0634\u0642\u0627 \u062c\u0627\u064a\u0644\u0627\u0631\u062f\u0649\u0643\u0649 \u0643\u0649\u0686\u0649\u0643 \u0643\u0627\u0631\u062e\u0627\u0646\u0649\u0644\u0627\u0631\u062f\u0649\u0646 \u062a\u06d5\u0631\u0643\u0649\u0628 \u062a\u0627\u067e\u0642\u0627\u0646 \u0628\u0648\u0644\u06c7\u067e \u060c \u062e\u06d5\u0644\u0642\u0626\u0627\u0631\u0627\u0644\u0649\u0642 \u062a\u0648\u0631 \u0633\u06d0\u062a\u0649\u06cb\u0627\u0644\u063a\u06c7\u0686\u0649\u0644\u0627\u0631\u063a\u0627 \u0645\u06d5\u06be\u0633\u06c7\u0644\u0627\u062a \u062a\u06d5\u0645\u0649\u0646\u0644\u06d5\u064a\u062f\u06c7.",
